<h1>Create A new Article</h1>
<%= form_with  scope: :article ,url: articles_path ,local: true do |f| %>
    <p>
        <%= f.label :title %><br/>
        <%= f.text_field :title %>

    </p>
    <p>
        <%=f.label :Description %><br/>
        <%=f.text_area :Description %>
    </p>
    <p>
        <%= f.submit %>
    </p>
    
<% end %>